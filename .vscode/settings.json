{
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "chat.tools.terminal.autoApprove": {
    "/.*/": true
  },
  "chat.tools.terminal.autoReplyToPrompts": true,
  "chat.tools.terminal.ignoreDefaultAutoApproveRules": true,
  "debug.toolBarLocation": "commandCenter",
  "editor.codeActionsOnSave": {
    "source.addMissingImports.ts": "always",
    "source.fixAll.eslint": "always",
    "source.removeUnusedImports": "never"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Fira Code, Ubuntu Mono, monospace",
  "editor.fontSize": 11,
  "editor.letterSpacing": -0.3,
  "editor.tabSize": 2,
  "eslint.enable": true,
  "eslint.format.enable": false,
  "eslint.probe": ["typescript", "typescriptreact", "html"],
  "eslint.useFlatConfig": true,
  "eslint.validate": ["typescript", "typescriptreact", "html"],
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/.buildcache/**": true,
    "**/.git/**": true,
    "**/node_modules/**": true
  },
  "files.watcherExclude": {
    "**/.buildcache/**": true,
    "**/.git/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true
  },
  "prettier.configPath": "prettier.config.mjs",
  "prettier.requireConfig": true,
  "scss.format.enable": false,
  "scss.lint.duplicateProperties": "error",
  "scss.lint.emptyRules": "warning",
  "scss.lint.propertyIgnoredDueToDisplay": "warning",
  "scss.lint.unknownAtRules": "warning",
  "scss.lint.unknownProperties": "warning",
  "scss.lint.zeroUnits": "warning",
  "scss.showErrors": true,
  "scss.validate": true,
  "scssFormatter.printWidth": 100,
  "scssFormatter.trailingComma": "es5",
  "search.exclude": {
    "**/.vscode/**": true,
    "**/dist/**": true,
    "*.json": true,
    "*.{ts,js,mjs}": true
  },
  "terminal.integrated.automationProfile.windows": {
    "args": ["-l"],
    "color": "terminal.ansiMagenta",
    "env": {
      "PATH": "${workspaceFolder}\\node_modules\\.bin;${env:PATH}",
      "TIMING": "1"
    },
    "path": "C:\\Program Files\\Git\\bin\\bash.exe"
  },
  "terminal.integrated.defaultProfile.windows": "bash-node",
  "terminal.integrated.fontFamily": "Hack, Fira Code, Ubuntu Mono, monospace",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.profiles.windows": {
    "bash-node": {
      "args": ["-l"],
      "color": "terminal.ansiCyan",
      "env": {
        "PATH": "${workspaceFolder}\\node_modules\\.bin;${env:PATH}",
        "TIMING": "1"
      },
      "icon": "terminal-bash",
      "overrideName": true,
      "path": "C:\\Program Files\\Git\\bin\\bash.exe"
    }
  },
  "terminal.integrated.shellIntegration.enabled": true,
  "typescript.format.enable": false,
  "typescript.preferences.importModuleSpecifier": "shortest",
  "typescript.tsc.autoDetect": "off",
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "vscode-icons"
}
